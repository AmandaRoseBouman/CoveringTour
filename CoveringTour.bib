	urldate = {2020-01-22},
	booktitle = {2019 {International} {Conference} on {Robotics} and {Automation} ({ICRA})},
	publisher = {IEEE},
	author = {Schenk, Fabian and Fraundorfer, Friedrich},
	month = may,
	year = {2019},
	pages = {154--160},
